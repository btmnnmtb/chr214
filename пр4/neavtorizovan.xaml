<Window x:Class="пр4.neavtorizovan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:пр4"
        mc:Ignorable="d"
        Title="neavtorizovan" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>

        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="exit" Content="Выйти" Grid.Column="0" Grid.Row="0" Click="exit_Click"/>
        <Button x:Name="redactor_testa" Content="Редактор" Grid.Column="1" Grid.Row="0" IsEnabled="False"/>
        <Button x:Name="proti" Content="Пройти тест" Grid.Column="2" Grid.Row="0"/>
        <DataGrid x:Name="MyData" Grid.Column="0" Grid.ColumnSpan="3"  Grid.Row="1" Grid.RowSpan="3"/>
    </Grid>
</Window>
